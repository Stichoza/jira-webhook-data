{
    "name": "kommuna/jirawebhook",
    "description": "PHP classes for Atlassian Jira webhook data structure with events",
    "keywords": ["jira", "atlassian", "webhook", "reflection"],
    "homepage": "https://github.com/kommuna/jirawebhook",
    "type": "library",
    "license": "Unlicense",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "fzaninotto/faker": "^1.9.1"
    },
    "autoload": {
        "psr-4": {
            "JiraWebhookDataStructures\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {
            "JiraWebhook\\Tests\\": "tests"
        }
    }
}
