{
    "name": "stichoza/jira-webhook-data",
    "description": "PHP classes for Atlassian Jira webhook data structures",
    "keywords": ["jira", "atlassian", "webhook", "reflection"],
    "homepage": "https://github.com/stichoza/jira-webhook-data",
    "type": "library",
    "license": "Unlicense",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "fzaninotto/faker": "^1.9.1"
    },
    "autoload": {
        "psr-4": {
            "JiraWebhookData\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JiraWebhookData\\Tests\\": "tests"
        }
    }
}
